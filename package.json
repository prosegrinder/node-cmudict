{
  "name": "@prosegrinder/cmudict",
  "version": "0.1.0",
  "type": "module",
  "description": "A versioned node wrapper for CMU Pronunciation Dictionary.",
  "exports": "./dist/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist/**/*",
    "types/index.d.ts"
  ],
  "scripts": {
    "test": "mocha --reporter spec ./tests/test.js",
    "clean": "rm -rf out/*",
    "build": "tsc && cp -r ./src/data ./dist/data",
    "rebuild": "npm run clean && npm run build",
    "lint": "eslint .",
    "version": "cz bump --changelog"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prosegrinder/node-cmudict.git"
  },
  "author": "David L. Day <david@prosegrinder.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/prosegrinder/node-cmudict/issues"
  },
  "homepage": "https://github.com/prosegrinder/node-cmudict#readme",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "chai": "^4.3.7",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.6.7",
    "mocha": "^10.2.0",
    "typescript": "^4.9.4"
  }
}
